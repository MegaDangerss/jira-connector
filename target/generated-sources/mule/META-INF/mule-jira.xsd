      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/jira" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="GetServerInfoType" substitutionGroup="mule:abstract-message-processor" name="get-server-info">
