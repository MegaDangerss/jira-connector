          <xs:annotation>
            <xs:documentation>Specify which configuration to use for this invocation.</xs:documentation>
          </xs:annotation>
        </xs:attribute>
        <xs:attribute xmlns="http://www.mulesoft.org/schema/mule/jira" type="longType" use="required" name="commentId">
          <xs:annotation>
            <xs:documentation>the commentId of the comment</xs:documentation>
          </xs:annotation>
        </xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:element xmlns="http://www.mulesoft.org/schema/mule/jira" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="GetConfigurationType" substitutionGroup="mule:abstract-message-processor" name="get-configuration">
