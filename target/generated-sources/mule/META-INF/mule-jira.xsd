  <xs:element xmlns="http://www.mulesoft.org/schema/mule/jira" xmlns:mule="http://www.mulesoft.org/schema/mule/core" type="GetIssuesFromFilterWithLimitType" substitutionGroup="mule:abstract-message-processor" name="get-issues-from-filter-with-limit">
